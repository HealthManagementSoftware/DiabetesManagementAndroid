<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToPadding="false"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".activities.ViewProfileActivity">

    <android.widget.TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:stretchColumns="1">

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:textStyle="bold"
                android:id="@+id/textView99"
                android:layout_width="113dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="80dp"
                android:text="First Name"
                android:textSize="16dp" />

            <TextView
                android:id="@+id/firstNameView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/first_name"
                android:textSize="16dp"
                tools:layout_editor_absoluteX="183dp" />

        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:textStyle="bold"
                android:layout_marginLeft="80dp"
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Last Name"
                android:textSize="16dp" />

            <TextView
                android:id="@+id/lastNameView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/last_name"
                android:textSize="16dp" />


        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:textStyle="bold"
                android:layout_marginLeft="80dp"
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@+id/textView99"
                android:text="Phone Number"
                android:textSize="16dp"
                tools:layout_editor_absoluteY="69dp" />

            <TextView
                android:id="@+id/phoneNumView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/phone_number"
                android:textSize="16dp"
                tools:layout_editor_absoluteX="183dp"
                tools:layout_editor_absoluteY="47dp" />

        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:textStyle="bold"
                android:layout_marginLeft="80dp"
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="City"
                android:textSize="16dp" />

            <TextView
                android:id="@+id/cityView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/city"
                android:textSize="16dp"
                tools:layout_editor_absoluteX="183dp"
                tools:layout_editor_absoluteY="69dp" />

        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:textStyle="bold"
                android:layout_marginLeft="80dp"
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@+id/textView99"
                android:text="State"
                android:textSize="16dp"
                app:layout_constraintBottom_toBottomOf="parent"
                tools:layout_editor_absoluteX="128dp" />

            <TextView
                android:id="@+id/stateView"
                android:layout_width="wrap_content"
                android:layout_height="24dp"
                android:layout_alignTop="@+id/textView99"
                android:text="@string/state"
                android:textSize="16dp"
                tools:layout_editor_absoluteX="183dp"
                tools:layout_editor_absoluteY="91dp" />

        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:textStyle="bold"
                android:layout_marginLeft="80dp"
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:text="Address"
                android:textSize="16dp"
                tools:layout_editor_absoluteX="108dp"
                tools:layout_editor_absoluteY="135dp" />

            <TextView
                android:id="@+id/addressView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:text="@string/address"
                android:textSize="16dp"
                tools:layout_editor_absoluteX="183dp"
                tools:layout_editor_absoluteY="113dp" />

        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:textStyle="bold"
                android:layout_marginLeft="80dp"
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="22dp"
                android:layout_centerInParent="true"
                android:text="Weight"
                android:textSize="16dp"
                tools:layout_editor_absoluteX="116dp"
                tools:layout_editor_absoluteY="157dp" />

            <TextView
                android:id="@+id/weightView"
                android:layout_width="wrap_content"
                android:layout_height="22dp"
                android:text="@string/weight"
                android:textSize="16dp"
                tools:layout_editor_absoluteX="183dp"
                tools:layout_editor_absoluteY="135dp" />

        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:textStyle="bold"
                android:layout_marginLeft="80dp"
                android:id="@+id/textView17"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Height"
                android:textSize="16dp"
                tools:layout_editor_absoluteX="119dp" />

            <TextView
                android:id="@+id/heightView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/height"
                android:textSize="16dp"
                tools:layout_editor_absoluteX="183dp"
                tools:layout_editor_absoluteY="157dp" />

        </TableRow>

    </android.widget.TableLayout>

</ScrollView>